	<queryString>
		<![CDATA[select *
from (--dato  alumno
	select a.ide_yaldap,descripcion_ystnac,descripcion_ysttis,descripcion_ystdoi,e.descripcion_ystesc,descripcion_ystgen,nombre_yaldap,apellido_yaldap,
	doc_identidad_yaldap,edad_yaldap,fecha_nac_yaldap,fotografia_yaldap,firma_yaldap,numero_yaltel,celular_yaltel,descripcion_yalcor,
	j.provincia||' - '||j.canton||' - '||j.parroquia as lugar_nacimiento,k.provincia as dir_provincia,k.canton as dir_canton,k.parroquia as dir_paroquia,descripcion_yaldia
	from yavirac_alum_dato_personal  a
	left join yavirac_stror_nacionalidad  b on a.ide_ystnac=b.ide_ystnac
	left join yavirac_stror_tipo_sangre c on a.ide_ysttis=c.ide_ysttis
	left join yavirac_stror_docu_identidad d on a.ide_ystdoi=d.ide_ystdoi
	left join yavirac_stror_estado_civil e on a.ide_ystesc=e.ide_ystesc
	left join yavirac_stror_genero f on a.ide_ystgen=f.ide_ystgen
	left join yavirac_alum_direccion_alumno g on a.ide_yaldap=g.ide_yaldap
	left join yavirac_alum_telefono h on a.ide_yaldap=h.ide_yaldap
	left join yavirac_alum_correo i on a.ide_yaldap=i.ide_yaldap
	left join view_division_politica j on a.ide_ystdip=j.ide_ystdip
	left join view_division_politica k on g.ide_ystdip=k.ide_ystdip
)a
left join (--datofamiliar
	select a.ide_yaldap,descripcion_yalgas as gasto,descripcion_yaltip as tipo_vivienda,m.descripcion_ystesc as estado_civi_familiar,descripcion_ystpaf as parentesco,
	o.descripcion_yalocu as ocupacion1,p.descripcion_yalocu as ocupacion2,q.descripcion_yalocu as ocupacion3,nombre_padre_yaldfa,edad_padre_yaldfa,
	nombre_madre_yaldfa, edad_madre_yaldfa, nombre_conyuge_yaldfa,edad_conyuge_yaldfa, numero_hijo_yaldfa, hermanos_yaldfa, puesto_ocupa_yaldfa
	from yavirac_alum_direccion_alumno a
	left join yavirac_alum_dato_fami_alumno j on a.ide_yaldap=j.ide_yaldap
	left join yavirac_alum_gasto k on j.ide_yalgas=k.ide_yalgas
	left join yavirac_alum_tipo_vivienda l on j.ide_yaltiv=l.ide_yaltiv
	left join yavirac_stror_estado_civil m on j.ide_ystesc=m.ide_ystesc
	left join yavirac_stror_parentezco_fami n on j.ide_ystpaf=n.ide_ystpaf
	left join yavirac_alum_ocupacion o on j.ide_yalocu=o.ide_yalocu
	left join yavirac_alum_ocupacion p on j.yav_ide_yalocu=p.ide_yalocu
	left join yavirac_alum_ocupacion q on j.yav_ide_yalocu2=q.ide_yalocu
) b on a.ide_yaldap=b.ide_yaldap
left join (--dato academico
	select ide_yaldap,b.descripcion_ystins as instutucion1,anio_graduacion_yaldaa,descripcion_ypetip as titulo,
	c.provincia||' - '||c.canton||' - '||c.parroquia as dir_institucion1,estudio_adicional_yaldaa,e.descripcion_ystins as instutucion2,
	descripcion_yalcar as carrera,anio_ingreso_yaldaa, anio_egreso_yaldaa,g.provincia||' - '||g.canton||' - '||g.parroquia as dir_institucion2
	from yavirac_alum_dato_academico a
	left join yavirac_stror_institucion b on a.ide_ystins=b.ide_ystins
	left join view_division_politica c on a.ide_ystdip=c.ide_ystdip
	left join yavirac_perso_titu_profesional d on a.ide_ypetip=d.ide_ypetip
	left join yavirac_stror_institucion e on a.yav_ide_ystins=e.ide_ystins
	left join yavirac_alum_carrera f on a.ide_yalcar=f.ide_yalcar
	left join view_division_politica g on a.yav_ide_ystdip=g.ide_ystdip
)c on a.ide_yaldap=c.ide_yaldap
left join (--curso o capacitacion
select ide_yaldap,descripcion_ystins as inst_capacitacion,observacion_yalcap,capacitacion_yalcap
from yavirac_alum_capacitacion a
left join yavirac_stror_institucion b on a.ide_ystins=b.ide_ystins
)d on a.ide_yaldap=d.ide_yaldap
left join yavirac_alum_dato_laboral e on a.ide_yaldap=e.ide_yaldap --dato laboral
left join (--dato medico
select ide_yaldap,padece_yaldam,c.descripcion_yalenf as enfermedad,tratamiento_yaldam,descripcion_yaltit as tipo_tratamiento,detalle_tratamiento_yaldam,
hospitalizado_yaldam,motivo_especializado_yaldam,operado_yaldam,motivo_operado_yaldam,enfermedad_cronica_yaldam,enfermedad_yaldam as enfermedad_cronica
from yavirac_alum_dato_medico a
left join yavirac_alum_tipo_tratamiento b on a.ide_yaltit=b.ide_yaltit
left join yavirac_alum_enfermedad c on a.ide_yalenf=c.ide_yalenf

) f on a.ide_yaldap=f.ide_yaldap
left join (-- alumno aficion
select ide_yaldap,actividad_yalafi,detalle_yalafi as aficion,descripcion_yaltia as tipo_aficion
from yavirac_alum_aficion a
left join yavirac_alum_tipo_aficion b on a.ide_yaltia=b.ide_yaltia
) g on a.ide_yaldap=g.ide_yaldap
left join (--familia emergencia
select ide_yaldap,nombre_yaldfe,descripcion_ystpaf as parenterco_emergencia,celular_yaldfe,telefono_yaldfe
from yavirac_alum_dato_fami_emerg a
left join yavirac_stror_parentezco_fami b on a.ide_ystpaf=b.ide_ystpaf
)h on a.ide_yaldap=h.ide_yaldap
where a.ide_yaldap=$P{ide_yaldap}]]>
	</queryString>
